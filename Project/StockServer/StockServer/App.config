<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>

    <add key="stockTickDelay" value="3500"/>
    
    <add key="cleanupDeadClients" value="true"/>

    <add key="defaultTimeout" value="4000"/>
    <add key="defaultRetryCount" value="3"/>
    
    <add key="brokerIp" value="127.0.0.1"/>
    <add key="brokerPort" value="5200"/>
    
    <add key="stockServerIp" value="127.0.0.1"/>
    <add key="stockServerPort" value="5300"/>
    <add key="stockServerTcpPort" value="5301"/>
    <add key="stockServerProcesNum" value="300"/>

  </appSettings>
</configuration>